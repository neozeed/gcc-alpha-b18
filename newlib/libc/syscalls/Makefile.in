srcdir = .

SHELL = /bin/sh

TOP = ../..
SRCTOP = ../..
TARGETDOC = ../targetdep.tex

#### Host, target, and site specific Makefile fragments come in here.
###

OFILES=\
	sysclose.o	\
	sysfcntl.o	\
	sysfork.o	\
	sysfstat.o	\
	sysgetpid.o	\
	syskill.o	\
	syslink.o	\
	syslseek.o	\
	sysopen.o	\
	sysread.o	\
	syssbrk.o	\
	sysstat.o	\
	sysunlink.o	\
	syswait.o	\
	syswrite.o

all: lib.a
lib.a: $(OFILES)
	rm -f $@
	$(AR) $(AR_FLAGS) $@ $(OFILES)

doc:

clean mostlyclean:
	$(RM) $(OFILES) *~ *.o.* lib.a

distclean maintainer-clean realclean: clean
	rm -f Makefile config.status

Makefile: Makefile.in $(host_makefile_frag) $(target_makefile_frag)
	$(SHELL) config.status


# to support SunOS VPATH
sysclose.o: sysclose.c
sysfcntl.o: sysfcntl.c
sysfork.o: sysfork.c
sysfstat.o: sysfstat.c
sysgetpid.o: sysgetpid.c
syskill.o: syskill.c
syslink.o: syslink.c
syslseek.o: syslseek.c
sysopen.o: sysopen.c
sysread.o: sysread.c
syssbrk.o: syssbrk.c
sysstat.o: sysstat.c
sysunlink.o: sysunlink.c
syswait.o: syswait.c
syswrite.o: syswrite.c
