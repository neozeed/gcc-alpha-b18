RANLIB_TEST = false
OLDCC = gcc
MAKE = make
SHELL = sh
CFLAGS= -O2 
CC = gcc
BISON=byacc
LEX=flex
# Don't run fixproto
STMP_FIXPROTO =
# Don't need collect2
USE_COLLECT2 =
exeext = .exe
INSTALL = ginstall
